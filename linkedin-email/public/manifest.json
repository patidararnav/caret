{
    "manifest_version": 3,
    "name": "LinkedIn Email Finder",
    "version": "1.0.0",
    "permissions": [
        "scripting",
        "activeTab"
    ],
    "background": {
        "service_worker": "serviceWorker.js",
        "type": "module"
    },
    "action": {
        "default_popup": "index.html"
    },
    "host_permissions": [
        "https://www.linkedin.com/*"
    ]
}
